{
    "env": {
        "browser": true,
        "es2020": true,
        "jest" : true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "jest"
    ],
    "rules": {
        "camelcase" : 1,
        "no-path-concat" : 0
    }
}
